            "type": "frser-sqlite-datasource",
            "uid": "ceracc5uxhibke"
          },
          "queryText": "SELECT cp.bank as current_bank, cp.type, cp.sub_type, cp.weighted_rate as current_rate, MAX(ap.aer_rate) as best_available_rate, (MAX(ap.aer_rate) - cp.weighted_rate) as rate_gap, COUNT(DISTINCT ap.bank_name) as alternative_banks FROM (SELECT bank, type, sub_type, SUM(balance * aer) / SUM(balance) as weighted_rate FROM my_deposits WHERE is_active = 1 AND balance IS NOT NULL AND aer IS NOT NULL GROUP BY bank, type, sub_type) cp LEFT JOIN available_products ap ON (cp.sub_type = 'Easy Access' AND ap.account_type IN ('easy_access', 'instant_access')) OR (cp.sub_type = 'Notice' AND ap.account_type IN ('notice', 'limited_access')) OR (cp.sub_type = 'Term' AND ap.account_type = 'fixed_term') WHERE ap.aer_rate IS NOT NULL AND ap.frn IS NOT NULL GROUP BY cp.bank, cp.type, cp.sub_type ORDER BY rate_gap DESC",
          "queryType": "table",
          "rawQueryText": "SELECT cp.bank as current_bank, cp.type, cp.sub_type, cp.weighted_rate as current_rate, MAX(ap.aer_rate) as best_available_rate, (MAX(ap.aer_rate) - cp.weighted_rate) as rate_gap, COUNT(DISTINCT ap.bank_name) as alternative_banks FROM (SELECT bank, type, sub_type, SUM(balance * aer) / SUM(balance) as weighted_rate FROM my_deposits WHERE is_active = 1 AND balance IS NOT NULL AND aer IS NOT NULL GROUP BY bank, type, sub_type) cp LEFT JOIN available_products ap ON (cp.sub_type = 'Easy Access' AND ap.account_type IN ('easy_access', 'instant_access')) OR (cp.sub_type = 'Notice' AND ap.account_type IN ('notice', 'limited_access')) OR (cp.sub_type = 'Term' AND ap.account_type = 'fixed_term') WHERE ap.aer_rate IS NOT NULL AND ap.frn IS NOT NULL GROUP BY cp.bank, cp.type, cp.sub_type ORDER BY rate_gap DESC",
          "refId": "A",
          "timeColumns": [
            "time",
            "ts"
          ]
        }
      ],
